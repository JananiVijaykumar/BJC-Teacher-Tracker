<div id="map"></div>

<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 39.50, lng: -98.35},
      zoom: 4
    });

    <% @items.each do |item| %>
      var marker = new google.maps.Marker({
        position: new google.maps.LatLng(<%= item[:long] %>, <%= item[:lat] %>),
      });
      marker.setMap(map);
    <% end %>
    
  }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC7jyOFHSorVb256ZEwvvyprp2KPjxKTPw&callback=initMap"
async defer></script>